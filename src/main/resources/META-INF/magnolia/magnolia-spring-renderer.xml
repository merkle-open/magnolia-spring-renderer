<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
    <name>magnolia-spring-renderer</name>
    <displayName>Magnolia spring-renderer</displayName>
    <description>The spring renderer module makes the Spring-Framework available for Magnolia</description>
    <versionHandler>com.merkle.oss.magnolia.renderer.spring.configuration.SpringRendererModuleVersionHandler</versionHandler>
    <version>${project.version}</version>

    <components>
        <id>main</id>
        <component>
            <type>com.merkle.oss.magnolia.renderer.spring.SpringRenderer</type>
            <implementation>com.merkle.oss.magnolia.renderer.spring.SpringRenderer</implementation>
        </component>
    </components>

    <dependencies>
        <dependency>
            <name>core</name>
        </dependency>
        <dependency>
            <name>rendering</name>
        </dependency>
    </dependencies>
</module>
